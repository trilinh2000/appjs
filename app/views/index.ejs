<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Image Uploading</title>
  </head>
<body>
    hello <%=account.username%>
    <form action="/home" enctype="multipart/form-data" ></form>
    <h1>Uploaded Images</h1>
    <div>
      <% items.forEach(function(image) { %>
          <img
          src="accounts:image/<%=image.img.contentType%>;base64,
          <%=image.img.data.toString('base64')%>"
          /> 
            <h5><%= image.name %></h5>
 
            <p><%= image.desc %></p>
      <% }) %>
    </div>
</body>
</html>